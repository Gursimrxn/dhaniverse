{
    "name": "dhaniverse",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "host": "vite --host",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "server": "deno run --allow-net --allow-env --allow-read --env-file=server/.env --watch server/index.ts",
        "server:ws": "deno run --allow-net --allow-env --allow-read --env-file=server/.env --watch server/src/websocket-server.ts",
        "server:local": "deno serve --allow-net --allow-env --allow-read --env-file=server/.env --port 8000 server/deploy.ts"
    },
    "devDependencies": {
        "@vitejs/plugin-react": "^4.3.4",
        "typescript": "~5.7.2",
        "vite": "^6.2.0"
    },
    "dependencies": {
        "@google-cloud/local-auth": "^3.0.1",
        "@types/node": "^22.14.1",
        "@types/react": "^19.1.0",
        "@types/react-dom": "^19.1.2",
        "autoprefixer": "^10.4.21",
        "clsx": "^2.1.1",
        "framer-motion": "^12.7.2",
        "google-auth-library": "^9.15.1",
        "lucide-react": "^0.488.0",
        "path": "^0.12.7",
        "phaser": "^3.88.2",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-router-dom": "^7.5.0",
        "tailwind-merge": "^3.2.0",
        "tailwindcss": "^3.4.11",
        "tailwindcss-animate": "^1.0.7"
    }
}
